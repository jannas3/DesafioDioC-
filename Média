using System;

namespace Start
{
    class Program
    {
        static void Main(string[] args)
        {
            double A, B, C, Media ;
            A = double.Parse(Console.ReadLine());
            B = double.Parse(Console.ReadLine());
            C = double.Parse(Console.ReadLine());
           
            Console.WriteLine( "A media eh {0:F1}",  Media=((A*2) + (B*3) + (C*5))/10);
            Console.ReadKey();
        }
    }
}
